= form_for [@coordinator, @event, @team] do |f|
  - if @team.errors.any?
    %h2= "#{pluralize(@team.errors.count, "error")} prohibited this team from being saved:"
    %ul
      - @team.errors.full_messages.each do |msg|
        %li= msg

  .field
    = f.label :name
    = f.text_field :name
  -#.field
    -# = f.label :event
    -# = f.text_field :event
  .actions
    = f.submit 'Save'
